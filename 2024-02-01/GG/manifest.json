{
  "manifest_version": 2,
  "name": "Hover Effect Extension_fast",
  "version": "3.4.3",
  "permissions": ["activeTab", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_icon": "./assets/img/icon-off.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["./assets/css/style.css"],
      "js": ["./assets/js/content_test.js"],
      "run_at": "document_end"
    }
  ]
}